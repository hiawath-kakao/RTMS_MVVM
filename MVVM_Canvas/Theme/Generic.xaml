    <Style x:Key="IOType" TargetType="{x:Type Label}">
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="FontSize" Value="9" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Label}">
                    <Border x:Name="Border" Background="{TemplateBinding Background}" CornerRadius="3" BorderThickness="0"
                                   BorderBrush="DarkGray" >
                        <ContentPresenter x:Name="MyContentPresenter"  Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        <!--Content="{TemplateBinding Content}"-->
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="Silver" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="False">
                            <Setter Property="Foreground" Value="White" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Foreground" Value="Silver" />
                            <Setter Property="Background" Value="Silver" />
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=Value.IO_TYPE}" Value="AD">
                            <Setter  Property="Background" Value="Orange"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Value.IO_TYPE}" Value="AD_STR">
                            <Setter  Property="Background" Value="Orange"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Value.IO_TYPE}" Value="DA">
                            <Setter  Property="Background" Value="LightGreen"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Value.IO_TYPE}" Value="DA_STR">
                            <Setter  Property="Background" Value="LightGreen"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Value.IO_TYPE}" Value="DI">
                            <Setter  Property="Background" Value="Aqua"/>
                        </DataTrigger>
                        <DataTrigger  Binding="{Binding Path=Value.IO_TYPE}" Value="DO">
                            <Setter  Property="Background" Value="LightSkyBlue"/>
                        </DataTrigger>
                        <DataTrigger  Binding="{Binding Path=Value.IO_TYPE}" Value="{x:Null}">
                            <Setter  Property="Background" Value="Red"/>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


